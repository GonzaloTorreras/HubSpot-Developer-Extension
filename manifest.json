{
	"manifest_version": 2,
	"name": "HubSpot Developer Extension ",
	"version": "1.0",
	"description": "A HubSpot Developers Sidekick Extension",
	"permissions": ["declarativeContent", "storage"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"cookies",
		"storage"
	]
}